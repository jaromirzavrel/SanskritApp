[MASTER]
ignore=venv
load-plugins=pylint.extensions.docparams

[MESSAGES CONTROL]
disable=C0114,C0115,C0116,W0611,W0612,R0903

[FORMAT]
max-line-length=100
indent-string='    '

[DESIGN]
max-args=5
max-locals=15
max-returns=6
max-branches=10
max-statements=50
max-line-length=100

[REPORTS]
output-format=colorized
